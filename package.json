{
  "name": "webpack-must-have",
  "version": "1.1.0",
  "description": "Must-have config for Webpack 5",
  "main": "main.jsx",
  "scripts": {
    "test": "npm run lint",
    "serve": "./node_modules/.bin/webpack serve --open --config webpack.devServer.js",
    "build-prod": "./node_modules/.bin/webpack --mode production --config webpack.prod.js",
    "build-prod-withMap": "./node_modules/.bin/webpack --mode production --config webpack.prod.js --sourceMap",
    "build-dev": "./node_modules/.bin/webpack --mode development --config webpack.dev.js",
    "lint": "npm run lint-es && npm run lint-styles && npm run lint-ts",
    "lint-ts": "./node_modules/.bin/tsc --skipLibCheck --incremental --noEmit",
    "lint-es": "./node_modules/.bin/eslint --fix --cache ./**/*.{js,jsx,ts,tsx,json}",
    "lint-styles": "./node_modules/.bin/stylelint --fix --cache ./**/*.{css,scss,less}",
    "lint-md": "./node_modules/.bin/markdownlint --fix **/*.md --ignore node_modules",
    "dev": "concurrently --kill-others \"npm run serve\" \"npm run json:server\"",
    "json:server": "json-server --watch db.json",
    "build": "react-scripts build",
    "postbuild": "find /path/to/build/static -type f \\( -name \\*.js -o -name \\*.css \\) -exec gzip {} \\; -exec mv {}.gz {} \\;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yegorich555/WebpackMustHave.git"
  },
  "keywords": [
    "React",
    "Webpack",
    "ReactJS",
    "Webpack4"
  ],
  "author": "Yegor.Golubchik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yegorich555/WebpackMustHave/issues"
  },
  "homepage": "https://github.com/Yegorich555/WebpackMustHave#readme",
